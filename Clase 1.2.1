---------------------------------------------------------------------------------

SELECT employee_id,
job_id,
last_name,
department_id,
commission_PCT
FROM employees
WHERE commission_PCT IS NULL;

---------------------------------------------------------------------------------

--IS NOT NULL--

---------------------------------------------------------------------------------

SELECT employee_id,
job_id,
last_name,
department_id,
commission_PCT
FROM employees
WHERE commission_PCT IS NOT NULL;

---------------------------------------------------------------------------------

